    "awilix": "^8.0.1",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.488.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.13.0",
    "react-icons": "^5.5.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.6.1",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "^1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "jest": "^29.7.0",
